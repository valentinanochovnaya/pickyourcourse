{
  "ConnectionStrings": {
    "DefaultConnection": "Server=redservo.duckdns.org;Port=5432;Database=pickyourcourse;User Id=postgres;Password=homeassistant;"
  },
  "SeriLog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId"],
    "WriteTo": [
      {"Name": "Console"},
      {"Name": "File",
        "Args": {
          "path": "logs.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:8081"
        }
      }
      
    ]
  },
  "AllowedHosts": "*"
}
