{
  "ConnectionStrings": {
    "DefaultConnection": "Server=dbmimi.postgres.database.azure.com;Database=dbmimi;Port=5432;User Id=mimi;Password=Sleepyhollow22;Trust Server Certificate=true;"
  },
  "SeriLog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId"],
    "WriteTo": [
      {"Name": "Console"},
      {"Name": "File",
        "Args": {
          "path": "logs.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:8081"
        }
      }
      
    ]
  },
  "AllowedHosts": "*"
}
